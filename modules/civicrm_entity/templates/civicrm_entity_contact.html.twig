{#
/**
 * @file civicrm_entity_contact.html.twig
 * Default theme implementation to present CiviCRM Contact entity data.
 *
 * This template is used when viewing a civicrm_entity_contact entity's page,
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_civicrm_entity_contact()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('civicrm_entity_contact') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
